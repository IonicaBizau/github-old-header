#!/usr/bin/env node

var Barbe = require("barbe")
  , Fs = require("fs")
  ;

const LIB_CONTENT = Fs.readFileSync(__dirname + "/../lib/index.js", "utf-8");
const EXT_CONTENT = Fs.readFileSync(__dirname + "/../extension/index.js", "utf-8");

Fs.writeFileSync(__dirname + "/../extension/index.js", Barbe(EXT_CONTENT, { lib: LIB_CONTENT }));
